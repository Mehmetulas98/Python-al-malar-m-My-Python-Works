def control(a,s):
    control=True
    operation_counter=0
    number=s

    while(control):

        if(number>a):
            operation_counter=0
            control=False
        if(number<=a):
            operation_counter+=1
            number+=s
            if(number>a):
                control=False
    return (operation_counter)

test_counter=input("Test Counter : ")
result_list=[]
for i in range(4):
    way_counter=0
    a = int(input("Ä°nput A : "))
    s = int(input("Ä°nput S : "))
    result_list.append(control(a,s))

print(result_list)







